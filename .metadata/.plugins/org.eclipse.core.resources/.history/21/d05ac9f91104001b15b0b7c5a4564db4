
public class PrincipalBiblioteca {

	public static void main(String[] args) {

		Biblioteca l1, l2;

		l1 = new Biblioteca();
		l2 = new Biblioteca();

		l1.setTitulo("Titulo Livro 1");
		l1.setAutor("Autor Livro 1");
		l1.setAno_publicacao(2001);
		l1.setCategoria("Categoria Livro 1");
		l1.setNum_estante(1);
		l1.setNum_prateleira(2);
		l1.setEmprestado(false);

		l2.setTitulo("Titulo Livro 2");
		l2.setAutor("Autor Livro 2");
		l2.setAno_publicacao(2002);
		l2.setCategoria("Categoria Livro 2");
		l2.setNum_estante(2);
		l2.setNum_prateleira(2);
		l2.setEmprestado(true);
		
		
		l1.imprimir_livro();

		System.out.println("");

		l2.imprimir_livro();
	}
}